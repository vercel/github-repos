{
  "name": "github-repos",
  "private": true,
  "version": "2.1.2",
  "scripts": {
    "start": "micro",
    "lint": "eslint .",
    "test": "yarn run lint",
    "lint:staged": "lint-staged"
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.js": [
      "eslint",
      "prettier --write --single-quote",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb",
      "prettier"
    ]
  },
  "dependencies": {
    "micro": "9.0.2",
    "ms": "2.1.1",
    "node-fetch": "1.7.3"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-config-airbnb": "17.0.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "2.12.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "lint-staged": "3.3.1",
    "pre-commit": "1.2.2",
    "prettier": "0.19.0"
  }
}
